{
  "kind": "collectionType",
  "collectionName": "user_profiles",
  "info": {
    "singularName": "user-profile",
    "pluralName": "user-profiles",
    "displayName": "UserProfile"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "username": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "password": {
      "type": "password",
      "required": true
    },
    "nickname": {
      "type": "string"
    },
    "email": {
      "type": "email"
    },
    "phone": {
      "type": "string",
      "minLength": 11,
      "maxLength": 11
    },
    "sex": {
      "type": "enumeration",
      "enum": [
        "man",
        "woman"
      ]
    }
  }
}
